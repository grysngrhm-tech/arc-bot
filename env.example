# ARC Bot Environment Configuration
# Copy this file to .env and fill in your values

# n8n Instance (self-hosted)
N8N_BASE_URL=https://your-n8n-instance.example.com
N8N_WEBHOOK_PATH=/webhook/arc-chat

# Supabase Project
SUPABASE_URL=https://your-project-id.supabase.co
SUPABASE_ANON_KEY=your-anon-key-here
SUPABASE_SERVICE_KEY=your-service-key-here

# OpenAI API
OPENAI_API_KEY=sk-your-api-key-here

# n8n Workflow IDs (for reference)
WORKFLOW_INGESTION_ID=your-ingestion-workflow-id
WORKFLOW_RETRIEVAL_ID=your-retrieval-workflow-id
WORKFLOW_AGENT_ID=your-agent-workflow-id

